    "entity:rollup": "yarn rollup",
    "ci:pipelines": "cross-env CI=true npm-run-all entity:*",
    "// - Utils": "Utilities",
    "sonar": "ts-node ./tools/sonar",
    "readme": "ts-node ./tools/copy-readme",
    "rollup": "rollup -c rollup.config.js",
    "bump": "ts-node ./tools/bump"
